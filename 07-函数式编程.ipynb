   "source": [
    "# sorted排序案例\n",
    "a = [100,30,20,-30,899]\n",
    "a1 = sorted(a,reverse=True) # 按照值的大小倒叙输出\n",
    "a2 = sorted(a,reverse=False)# 按照值的大小顺序输出\n",
    "al = sorted(a,key=abs, reverse=True)# 按照绝对值的大小倒叙输出\n",
    "\n",
    "\n",
    "print(a1)\n",
    "print(a2)\n",
    "print(al)"
